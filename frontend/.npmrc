# Skip optional dependencies that cause build issues
optional=false

# Ignore scripts for problematic packages (unrs-resolver, protobufjs)
# These are dev dependencies and work fine without postinstall scripts
ignore-scripts=false

# Use legacy peer deps to avoid conflicts
legacy-peer-deps=false

# Skip postinstall for unrs-resolver (Rust-based, can fail in some environments)
# It's only used by eslint-import-resolver-typescript and works without the script
